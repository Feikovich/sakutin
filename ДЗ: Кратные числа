using System;

namespace MyProject  
{
    class Program    
    {
        static void Main(string[] args)  
        {
            Random random = new Random();
            int upperLimit = 25;
            int lowerlimit = 10;
            int minimumNumber = 50;
            int maximumNumber = 150;
            int countOfMultipleNumbers = 0;
            int randomNumber = random.Next(lowerlimit, upperLimit + 1);
            int firstMultipleNumber = 0;

            while(firstMultipleNumber <= minimumNumber)
            {
                firstMultipleNumber += randomNumber;
            }

            while(firstMultipleNumber <= maximumNumber)
            {
                firstMultipleNumber += randomNumber;
                countOfMultipleNumbers++;
            }
            Console.WriteLine(randomNumber);

            Console.WriteLine(countOfMultipleNumbers);
        }
    }
}
