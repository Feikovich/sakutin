using System;

namespace MyProject  
{
    class Program    
    {
        static void Main(string[] args)  
        {
           string cupOfCoffee = "Tea";
           string cupOfTea = "Coffee";
           string tempCup = cupOfTea;
           cupOfTea = cupOfCoffee;
           cupOfCoffee = tempCup;

            System.Console.WriteLine(cupOfCoffee);
            System.Console.WriteLine(cupOfTea);

        }
    }
}







