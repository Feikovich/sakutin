using System;
using System.Runtime.InteropServices;

namespace MyProject  
{
    class Program    
    {
        static void Main(string[] args)  
        {
            Random random = new Random();
            int upperLimit = 25;
            int lowerlimit = 10;
            int minimumNumber = 50;
            int maximumNumber = 150;
            int countOfMultipleNumbers = 0;
            int randomNumber = random.Next(lowerlimit,upperLimit + 1); 

            for (int i = minimumNumber;i <= maximumNumber; i++)
            {
                int checkMultipleNumber = i;

                while(checkMultipleNumber >= randomNumber)
                {
                    checkMultipleNumber -= randomNumber;
                }

                if(checkMultipleNumber == 0)
                {
                    countOfMultipleNumbers++ ;
                }
            }
            Console.WriteLine(randomNumber);
            Console.WriteLine(countOfMultipleNumbers);


        }
    }
}
