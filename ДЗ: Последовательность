using System;

namespace MyProject  
{
    class Program    
    {
        static void Main(string[] args)  
        {
            int maxNumber = 110;
            int currentNumber;
            int startNumber = 5;
            int stepOfNumber = 7;
            
            for(currentNumber = startNumber; currentNumber < maxNumber; currentNumber += stepOfNumber)
            {
                Console.WriteLine(currentNumber);
            }
        }
    }
}
